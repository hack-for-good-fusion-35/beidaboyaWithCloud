
.tBar {
  margin: 28rpx 0 20rpx 28rpx;
  font-size: 28rpx;
}

.warp {
  background-color: #fff;
  border-width: 2rpx 0 2rpx 0;
  border-color: #d8d8d8;
  border-style: solid;
  margin-bottom: 18rpx;
}

.submit-btn {
  background-color: #f76968;
  position: fixed;
  bottom: 0;
  width: 100%;
  text-align: center;
  line-height: 94rpx;
  color: #fff;
  font-size: 36rpx;
  border-radius:0px;
}

